{
  "name": "openloveimage",
  "version": "0.1.3",
  "private": true,
  "description": "Modern image converter with before/after preview - Convert HEIC, JPG, PNG, WebP, GIF, BMP, TIFF, SVG with advanced features",
  "repository": {
    "type": "git",
    "url": "https://github.com/ireddragonicy/openloveimage.git"
  },
  "homepage": "https://github.com/ireddragonicy/openloveimage#readme",
  "bugs": {
    "url": "https://github.com/ireddragonicy/openloveimage/issues"
  },
  "author": "ireddragonicy",
  "keywords": [
    "image-converter",
    "heic",
    "nextjs",
    "react",
    "typescript",
    "material-ui"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "release:windows": "tauri build --target x86_64-pc-windows-msvc",
    "release:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "release:macos": "tauri build --target universal-apple-darwin",
    "release:all": "npm run release:windows && npm run release:linux && npm run release:macos",
    "version:patch": "npm version patch --no-git-tag-version && npm run version:sync",
    "version:minor": "npm version minor --no-git-tag-version && npm run version:sync",
    "version:major": "npm version major --no-git-tag-version && npm run version:sync",
    "version:sync": "node scripts/sync-version.js",
    "changelog": "node scripts/generate-changelog.js",
    "clean": "node scripts/clean.js",
    "clean:all": "node scripts/clean.js --all",
    "check": "npm run lint && npm run build",
    "prepare:release": "npm run check && npm run changelog",
    "dev:clean": "npm run clean && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/system": "^7.1.1",
    "2ico": "^1.1.2",
    "fabric": "^6.7.0",
    "file-saver": "^2.0.5",
    "heic-to": "^1.1.14",
    "imagetracerjs": "^1.2.6",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "konva": "^9.3.20",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-image-crop": "^11.0.10",
    "react-konva": "^19.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tauri-apps/cli": "^2.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
